   (state <s> ^operator <op> +
   	      ^interaction.stack.segment <top>
	      ^object <obj>)
   (<obj> ^shape <obj-name>
   	  ^id <obj-id>)
   (<top> ^originator instructor
   	  ^purpose <p>)
   (<p> ^type environmental-action
   	^satisfaction <ps>)
   (<ps> ^action-event <info>)
   (<info> ^word pick
   	   ^preposition.object.word <obj-name>)
   (<op> ^name pick-up-object
   	 ^actions.pick-up.object-id <obj-id>)
-->
   (<s> ^operator <op> >)
   (<op> ^executed-purpose <p>)
}

