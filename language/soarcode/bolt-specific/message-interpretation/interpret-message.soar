sp {interpret-message*prefer*verb-command-over-any
   (state <s> ^name interpret-message
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name interpret-verb-command)
   (<o2> ^kind message-interpretation
        -^name interpret-primitive-object
        -^name interpret-verb-command)
