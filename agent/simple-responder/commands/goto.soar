sp {apply*process-message*goto
   (state <s> ^name responder
              ^operator <o>
              ^io.input-link.message <message>)
   (<o> ^name process-message
        ^message <msgID>)
   (<message> ^id <msgID>
          ^words <w1>)
   (<w1> ^word goto
         ^next <w2>)
   (<w2> ^word <x>
         ^next <w3>)
   (<w3> ^word <y>
         ^next <w4>)
   (<w4> ^word <z>
         -^next)
-->
   (<s> ^desired <d>)
   (<d> ^message <msgID>
        ^location <l>)
   (<l> ^x <x>
        ^y <y>
        ^z <z>)
}

sp {goto*propose*goto*location
   (state <s> ^desired <d>)
   (<d> ^location <loc>
       -^status << complete error >>)
   (<loc> ^x <x> ^y <y> ^z <z>)
-->
   (<s> ^operator <o> +)
   (<o> ^name goto
        ^actions.goto.location <loc>)
   (<loc> ^x <x> ^y <y> ^z <z>)
}