sp {apply*process-message*goto*1
   (state <s> ^name responder
              ^operator <o>
              ^io.input-link.messages.message <message>)
   (<o> ^name process-message
        ^message <msgID>)
   (<message> ^id <msgID>
          ^words <w1>)
   (<w1> ^first-word goto
         ^rest <w2>)
   (<w2> ^first-word <place>
         -^rest.first-word)
-->
   (<s> ^instructions <i>)
   (<i> ^type goto
        ^message <msgID>
        ^place <place>)
}

