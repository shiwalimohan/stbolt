sp {multiple*argument*matches*create*operator*composite-operator
   (state <s> ^superstate <ss>
   	     -^learning-operator <any>
             -^status.type which-question)
   (<ss> ^indexed-operator <iop>
	 ^resolve-operator <rop>
	 ^operator.category composite-action)
   (<iop> ^<arg> <val1>
	  ^<arg> {<val2> <> <val1>}
	  ^_metadata.arg-data.argument <arg>)	
-->
   (<rop> ^name resolve-map-impasse
	  ^indexed-operator <iop>
	  ^category indexing
	  ^argument <arg>
	  ^map-impasse-type multiple-argument-match)
   (<ss> ^operator <iop> -)
}

sp {multiple*argument*matches*apply*select-object
   (state <s> ^name resolve-map-impasse
   	      ^operator <op>
	      ^argument <arg>
	      ^indexed-operator <iop>
	      ^map-impasse-type multiple-argument-match
	      ^superstate <ss>)
   (<op> ^name select-object)
   (<ss> ^resolve-operator <rop>)
-->
   (<ss> ^resolve-operator <rop> -
   	 ^resolve-operator <nrop>)  
}