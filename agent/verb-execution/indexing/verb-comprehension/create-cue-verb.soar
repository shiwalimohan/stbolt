### 
sp {create*cue*verb
   (state <s> ^name create-cue
   	      ^superstate.information <info>
	      ^topstate.interaction.stack.segment.indexing-status <stat>)
-->
   (<s> ^information <info>
   	^bit flip
	^status <stat>)
}

sp {elaborate*verb
   (state <s> ^name create-cue
   	      ^information <info>)
   (<info> ^verb.word <verb>)
-->
   (<s> ^cue <cue>)
   (<cue> ^verb <verb>)
}

sp {elaborate*arguments*direct-object
   (state <s> ^name create-cue
   	      ^information.verb <info>)
   (<info> ^direct-object.object <do>)
-->
   (<s> ^argument <do>)
}

sp {elaborate*arguments*pp-object
   (state <s> ^name create-cue
   	      ^information.verb <info>)
   (<info> ^preposition.object <do>)
-->
   (<s> ^argument <do>)
}

sp {propose*index-argument*direct-object
   (state <s> ^name create-cue
   	      ^information.verb <info>
	      ^cue <cue>
	      -^matched <do>
	      ^bit flip
	      ^status <stat>)
   (<stat> -^direct-object <any>)
   (<info> ^direct-object.object <do>)
   (<cue> -^direct-object <any>)
-->
   (<s> ^operator <op> + >)
   (<op> ^name index-argument
   	 ^cue <cue>
	 ^connector direct-object
	 ^argument <do>
	 ^category indexing
	 ^context-id direct-object
	 ^return-all true
	 ^allow-ties false
	 ^status <stat>)
}


sp {propose*index-argument*preposition
   (state <s> ^name create-cue
   	      ^information.verb <info>
	      ^bit flip
	      -^_relation.preposition <prep>
	      ^status <stat>)
   (<stat> -^_relation <any>)
   (<info> ^preposition.word <prep>)
  -->
   (<s> ^operator <op> + =)
   (<op> ^name index-preposition
	 ^preposition <prep>
	 ^category indexing
	 ^status <stat>)
}

sp {propose*index-argument*preposition*object
   (state <s> ^name create-cue
   	      ^information.verb <info>
	      ^cue <cue>
	      -^matched <obj>
	      ^bit flip
	      ^_relation <rel>
	      ^status <stat>)
   (<stat> -^<p> <any>)
   (<info> ^preposition.object <obj>)
   (<rel> ^preposition <p>)
   (<cue> -^<p> <any>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name index-argument
   	 ^cue <cue>
	 ^argument <obj>
	 ^connector <p>
	 ^category indexing
	 ^return-all true
	 ^allow-ties false
	 ^context-id <p>
	 ^status <stat>)
}

