sp {propose*write*matched*objects*perceptual*superstate
   (state <s> ^name index-argument
   	      ^match <obj>
	      -^topstate.interaction.referents.objects.object <obj>)
-->
   (<s> ^operator <op> +)
   (<op> ^name activated-referents
   	 ^category indexing)
}

sp {preference*focus-referents*activated-referents
   (state <s> ^name index-argument
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name focus-referents)
   (<op2> ^name activated-referents)
-->
   (<s> ^operator <op1> > <op2>)
}


