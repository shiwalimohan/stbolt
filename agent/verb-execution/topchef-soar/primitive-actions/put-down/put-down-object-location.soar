### put down an object in a location

sp {propose*put-down-object-location
#   (state <s> ^self.grabbed-object <id> <> -1
    (state <s> ^grabbed <obj>
	      ^location <loc>
	       -^problem-space << lgsoar message-interpretation >>)
-->
   (<s> ^operator <op> +)
   (<op> ^name op_put-down-object-location
   	 ^argument1 <obj>
	 ^argument2 <loc>
	 ^category primitive-action)
}

sp {elaborate*put-down*location*location
   (state <s> ^operator <op> +)
   (<op> ^name op_put-down-object-location
   	 ^argument1 <obj>
	 ^argument2 <loc>)
-->
   (<op> ^actions.put-down.location <loc>		 
   	 ^category primitive-action
	 ^success yes)
   (<op> ^kind non-atomic)
}

### action model
sp {apply*put-down*location*action-model
   (state <s> ^operator <op>	
	     -^io
	     ^grabbed <obj>)
   (<op> ^name op_put-down-object-location
   	 ^argument1 <obj>
	 ^argument2 <loc>)
   (<loc> ^x <s_x>
   	  ^y <s_y>
	  ^z <s_z>)
-->
   (<s> ^grabbed <obj> -)
   (<obj>    ^x <s_x>
   	     ^y <s_y>
	     ^z <s_z>)
}




