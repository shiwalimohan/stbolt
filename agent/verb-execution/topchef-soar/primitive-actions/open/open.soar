## propose open location for all closed locations
sp {propose*open*location
   (state <s> ^object <obj>
   	       -^problem-space << lgsoar message-interpretation >>
	       ^topstate <ts>
	       ^closed <obj>)
 -->
   (<s> ^operator <op> +)
   (<op> ^name op_open-location
   	 ^argument1 <obj>)
}

sp {elaborate*open*location*action
   (state <s> ^operator <op> +)
   (<op> ^argument1 <obj>
   	 ^name op_open-location)
   (<obj> ^id <id>)
  -->
   (<op> ^actions.set-state <point>
 	 ^category primitive-action)
   (<point> ^id <id>
   	    ^name door
   	    ^value open)
   (<op> ^kind non-atomic)
}

