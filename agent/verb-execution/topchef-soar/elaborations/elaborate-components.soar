#### elaborate x,y,z for objects/locations

sp {elaborate*xyz
   (state <s> ^<< object location self >> <l>
   	      ^io)
   (<l> ^pose <lp>)
   (<lp> ^x <x>
   	 ^y <y>
	 ^z <z>)
-->
   (<l> ^x <x>
   	^y <y>
	^z <z>)
}
