### find the next operator indexed based on language after a given id
sp {find-next-indexed-operator*elaborate*state
   (state <s> ^name find-next-indexed-operator
   	      ^superstate.operator <sop>)
   (<sop> ^name find-next-indexed-operator
   	  ^after-id <id>)
-->
   (<s> ^after-id <id>)
}

sp {find-next-operator*propose*set-id
   (state <s> ^name find-next-indexed-operator
   	      -^id 
   	      ^after-id <id>)
-->
   (<s> ^operator <op> + <)
   (<op> ^name set-id
   	 ^id <id>)
}

sp {find-next-operator*apply*set-id
   (state <s> ^name find-next-indexed-operator
   	      ^operator <op>)
   (<op> ^name set-id
   	 ^id <id>)
-->
   (<s> ^id <id>)
}

sp {find-next-operator*propose*retrieve*episode
   (state <s> ^name find-next-indexed-operator
   	      ^epmem.command <ecomm>
	      ^id <id>)
   -->
   (<ecomm> ^retrieve <id>)
}

