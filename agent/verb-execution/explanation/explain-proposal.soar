### propose an operator if it is not one of the matching primitives

sp {create*no-match*operator
   (state <s> ^name selection
   	      ^indexed-operator <map-op>
	      ^superstate <ss>)
   (<map-op> ^name <map-name>
    	      ^<arg1> <iobj1>
	      ^{<arg2> <> <arg1>} <iobj2>
	      ^_metadata.map-id <m1>
	      ^category composite-action)
   (<iobj1> ^id )
   (<iobj2> ^id )
   (<ss> ^object <obj1>
    	 ^object <obj2> <> <obj1>)
   (<obj1> ^focus <any1>)
   (<obj2> ^focus <any2>)
-->
   (<s> ^superstate-propose-operator <spo>)
   (<spo> ^name <map-name>
   	  ^category composite-action
	  ^<arg1> <obj1>
	  ^<arg2> <obj2>
	  ^_metadata.map-id <m1>)
}

sp {propose*no-match*operator*evaluate*operator
   (state <s> ^name selection
   	      ^superstate-propose-operator <spo>
	      ^superstate <ss>)
-->
   (<ss> ^operator <spo> +)
}

### if evaluation is successful; propose the operator with highest preference
sp {propose*operator*superstate
   (state <s> ^name evaluate-operator
   	      ^duplicate-of <ss>
	      ^superstate.operator.superoperator <spo>
	      ^desired <d>
	      ^success <d>)
-->
   (<ss> ^operator <spo> + >)
}
