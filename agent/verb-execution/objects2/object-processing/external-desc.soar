sp {object-processing*elaborate*external-desc
   (state <s> ^topstate <s>
              ^object <obj>)
-->
   (<obj> ^external-desc <desc>)
}

sp {object-processing*elaborate*external-desc*name
   (state <s> ^topstate <s>
              ^object <obj>)
   (<obj> ^external-desc <desc>
          ^property <prop>)
   (<prop> ^category name
           ^word <name>)
-->
   (<desc> ^name <name>)
}

sp {object-processing*elaborate*external-desc*adjective
   (state <s> ^topstate <s>
              ^object <obj>)
   (<obj> ^external-desc <desc>
          ^property <prop>)
   (<prop> ^category {<cat> <> name}
           ^word <word>)
-->
   (<desc> ^adjective <word>)
}