sp {propose*push-object-desc
   (state <s> ^topstate <s>
              ^instructor-message.type object-identification)
-->
   (<s> ^operator <o> +)
   (<o> ^name push-object-desc
        ^category object-processing)
}

sp {apply*push-object-desc
   (state <s> ^operator.name push-object-desc
              ^instructor-message <msg>)
   (<msg> ^information.object <obj>)
-->
   (<msg> ^purpose <p>)
   (<p> ^type object-desc
        ^parameters <params>
        ^satisfaction <sat>)
   (<params> ^object <obj>)
   (<sat> ^learning-event object)
}