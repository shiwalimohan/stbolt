# Inside an object's mapping, adds labels to the att-val-pair.att and att-val-pair.val identifiers
# Adds either ^att-type or ^val-type, which can take on the following values:
#	unknown - no internal or external symbols
#	internal - only the internal symbol
#	external - only the external symbol
#	matched - both the internal and external symbols

sp {object-elaborations*elaborate*att-type*matched
   (state <s> ^superstate nil
              ^object.mapping.att-val-pair <av-pair>)
   (<av-pair> ^att <att>)
   (<att> ^internal
          ^external)
-->
   (<av-pair> ^att-type matched)
}

sp {object-elaborations*elaborate*att-type*internal
   (state <s> ^superstate nil
              ^object.mapping.att-val-pair <av-pair>)
   (<av-pair> ^att <att>)
   (<att> ^internal
          -^external)
-->
   (<av-pair> ^att-type internal)
}

sp {object-elaborations*elaborate*att-type*external
   (state <s> ^superstate nil
              ^object.mapping.att-val-pair <av-pair>)
   (<av-pair> ^att <att>)
   (<att> -^internal
          ^external)
-->
   (<av-pair> ^att-type external)
}

sp {object-elaborations*elaborate*att-type*unknown
   (state <s> ^superstate nil
              ^object.mapping.att-val-pair <av-pair>)
   (<av-pair> ^att <att>)
   (<att> -^internal
          -^external)
-->
   (<av-pair> ^att-type unknown)
}

sp {object-elaborations*elaborate*val-type*matched
   (state <s> ^superstate nil
              ^object.mapping.att-val-pair <av-pair>)
   (<av-pair> ^val <val>)
   (<val> ^internal
          ^external)
-->
   (<av-pair> ^val-type matched)
}

sp {object-elaborations*elaborate*val-type*internal
   (state <s> ^superstate nil
              ^object.mapping.att-val-pair <av-pair>)
   (<av-pair> ^val <val>)
