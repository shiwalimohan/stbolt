# Prefer external comprehend-att > ask-query > internal comprehend-att
sp {prefer*external*comprehend-att*over*ask-query
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name comprehend-att
         ^type external)
   (<o2> ^name ask-query)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {prefer*ask-query*over*internal*comprehend-att
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name ask-query)
   (<o2> ^name comprehend-att
         ^type internal)
-->
   (<s> ^operator <o1> > <o2>)
}


