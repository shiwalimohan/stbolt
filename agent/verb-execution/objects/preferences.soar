#######------ object-processing ------#####
sp {elaborate*pref-rank*create-object
   (state <s> ^topstate <s>
              ^operator <o> +)
   (<o> ^name delete-object)
-->
   (<o> ^pref-rank 0)
}

# Rank 1: create-object
sp {elaborate*pref-rank*create-object
   (state <s> ^topstate <s>
              ^operator <o> +)
   (<o> ^name create-object)
-->
   (<o> ^pref-rank 1)
}

# Rank 2: learn-att learn-val
sp {elaborate*pref-rank*learning
   (state <s> ^topstate <s>
              ^operator <o> +)
   (<o> ^name << learn-att learn-val learn-val2att >> )
-->
   (<o> ^pref-rank 2)
}

