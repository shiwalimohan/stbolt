# smem-val-query
#   Queries smem for the matching value

sp {lookup-val2att*propose*smem-val2att-query
   (state <s> ^name lookup-val2att
              ^value <val>
              -^smem.command.query.type val2att)
-->
   (<s> ^operator <o> +)
   (<o> ^name smem-val2att-query
        ^category object-processing
        ^value <val>)
}

sp {lookup-val2att*apply*smem-val2att-query
   (state <s> ^name lookup-val2att
              ^operator <o>
              ^smem.command <cmd>)
   (<o> ^name smem-val2att-query
        ^value <val>)
-->
   (<cmd> ^query <q>)
   (<q> ^type val2att
        ^value <val>
        ^attribute <att>)
}
