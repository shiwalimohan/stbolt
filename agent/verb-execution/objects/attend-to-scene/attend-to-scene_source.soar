sp {propose*attend-to-scene
   (state <s> ^topstate <s>
              ^name
             -^attended)
-->
   (<s> ^operator <o> + >)
   (<o> ^name attend-to-scene
        ^category object-processing)
}

sp {apply*attend-to-scene
   (state <s> ^topstate <s>
              ^operator <o>)
   (<o> ^name attend-to-scene
        ^status complete)
-->
   (<s> ^attended true)
}

