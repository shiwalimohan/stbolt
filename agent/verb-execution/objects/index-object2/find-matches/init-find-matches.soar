# init-identify-operator
#   Creates the object-matches structure that will eventally be returned
sp {find-matches*propose*init-find-matches
   (state <s> ^name find-matches
             -^object-matches)
-->
   (<s> ^operator <o> +)
   (<o> ^name init-find-matches
        ^category indexing)
}

sp {find-matches*apply*init-find-matches
   (state <s> ^name find-matches
              ^operator.name init-find-matches
              ^superstate.operator.object <obj>)
-->
   (<s> ^object-matches.object <obj>)
}

sp {find-matches*apply*init-find-matches*candidate-set
   (state <s> ^name find-matches
              ^operator.name init-find-matches
              ^superstate.operator.candidate-set <cs>)
-->
   (<s> ^candidate-set <cs>)
}
   