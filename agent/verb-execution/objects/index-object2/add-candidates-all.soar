# ALL CANDIDATES
sp {index-object*elaborate*object-to-index*use-all
   (state <s> ^name index-object
              ^object-to-index <obj>)
-->
   (<obj> ^use-all true)
}

sp {index-object*propose*add-candidates*all
   (state <s> ^name index-object
              ^object-to-index <obj>)
   (<obj> ^use-all true
          -^did-all)
-->
   (<s> ^operator <o> + =)
   (<o> ^name add-candidates
        ^category indexing
        ^candidate-type all
        ^object-to-index <obj>)
}

sp {index-object*apply*add-candidates*all
   (state <s> ^name index-object
              ^operator <o>)
   (<o> ^name add-candidates
        ^candidate-type all
        ^object-to-index <obj>)
-->
   (<obj> ^did-all true)
}

sp {index-object*apply*add-candidates*all*add*candidates
   (state <s> ^name index-object
              ^operator <o>
              ^topstate.object <obj>)
   (<o> ^name add-candidates
        ^candidate-type all
        ^object-to-index.candidates <c>)
   (<c> -^candidate.object <obj>)
-->
   (<c> ^candidate <candidate>)
   (<candidate> ^object <obj>
                ^pref 1000)
}