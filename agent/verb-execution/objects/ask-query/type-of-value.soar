# type-of-value
#	Query for the attribute of a known value

sp {elaborate*ask-query*type-of-value
   (state <s> ^operator <o>)
   (<o> ^name ask-query
        ^query.type type-of-value)
-->
   (<o> ^type type-of-value
        ^expected-response bare-attribute-response)
}

sp {elaborate*ask-query*type-of-value*value
   (state <s> ^operator <o>)
   (<o> ^name ask-query
        ^query <q>
        ^fields <f>)
   (<q> ^type type-of-value
        ^value <v>)
-->
   (<f> ^value <v>)
}