sp {find-object*propose*no-match
   (state <s> ^name find-object)
-->
   (<s> ^operator <o> +)
   (<o> ^name no-match
        ^category object-processing)
}

sp {find-object*prefer*object-match*no-match
   (state <s> ^name find-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name no-match)
   (<o2> ^name object-match)
-->
   (<s> ^operator <o2> > <o1>)
}

sp {find-object*apply*no-match*mark*failure
   (state <s> ^name find-object
              ^operator.name no-match
              ^superstate.operator <ss-o>)
-->
   (<ss-o> ^match failure)
}