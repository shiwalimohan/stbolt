sp {propose*init-learn-att
   (state <s> ^name learn-att
             -^learned-att)
-->
   (<s> ^operator <o> +)
   (<o> ^name init-learn-att)
}

sp {apply*init-learn-att
   (state <s> ^name learn-att
              ^operator.name init-learn-att
              ^superstate.learned-att <la>)
-->
   (<s> ^learned-att <la>)
}