sp {propose*init-comprehend-att
   (state <s> ^name comprehend-att
             -^att-to-comp 
              ^superstate.operator <ss-o>)
   (<ss-o> ^attribute <att>
           ^type <att-type>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name init-comprehend-att
        ^attribute <att>
        ^type <att-type>)
}

sp {apply*init-comprehend-att*attribute
   (state <s> ^name comprehend-att
              ^operator <o>)
   (<o> ^name init-comprehend-att
        ^attribute <att>
        ^type <att-type>)
-->
   (<s> ^att-to-comp <att>
        ^att-type <att-type>)
}

sp {apply*init-comprehend-att*objects
   (state <s> ^name comprehend-att
              ^operator <o>
              ^superstate.operator.object <obj>)
   (<o> ^name init-comprehend-att
        ^attribute <att>)
-->
   (<s> ^object <obj>)
}