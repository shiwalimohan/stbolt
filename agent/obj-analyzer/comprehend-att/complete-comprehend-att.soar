sp {propose*complete-comprehend-att
   (state <s> ^name comprehend-att)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-comprehend-att)
}

sp {apply*complete-comprehend-att
   (state <s> ^name comprehend-att
              ^operator.name complete-comprehend-att
              ^att-to-comp <att>
              ^superstate.attributes <atts>)
-->
   (<atts> ^processed <att>)
}

sp {apply*complete-comprehend-att*remove*unprocessed-att
   (state <s> ^name comprehend-att
              ^operator.name complete-comprehend-att
              ^att-to-comp <att>
              ^superstate.attributes <atts>)
   (<atts> ^unprocessed <att>)
-->
   (<atts> ^unprocessed <att> -)
}
