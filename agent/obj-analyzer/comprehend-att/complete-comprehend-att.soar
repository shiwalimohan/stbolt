sp {propose*complete-comprehend-att
   (state <s> ^name comprehend-att)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-comprehend-att)
}

sp {apply*complete-comprehend-att
   (state <s> ^name comprehend-att
              ^operator.name complete-comprehend-att
              ^att-type <att-type>
              ^att-to-comp <att>
              ^superstate <ss>)
-->
   (<ss> ^processed.<att-type> <att>)
}

