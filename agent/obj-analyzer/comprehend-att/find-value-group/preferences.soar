sp {prefer*all*over*complete-find-value-group
   (state <s> ^name find-value-group
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name <> complete-find-value-group)
   (<o2> ^name complete-find-value-group)
-->
   (<s> ^operator <o1> > <o2>)
}


sp {prefer*shared-att-val*over*differs-from-group
   (state <s> ^name find-value-group
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name shared-att-val)
   (<o2> ^name differs-from-group)
-->
   (<s> ^operator <o1> > <o2>)
}