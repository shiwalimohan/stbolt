sp {elaborate*object*unknown-att
   (state <s> ^name obj-analyzer
              ^object.mapping <mapping>)
   (<mapping> ^att-val-pair.att <att>)
   (<att> ^internal <internal-att>
         -^external <external-att>)
-->
   (<mapping> ^unknown-att <internal-att>)
}

sp {elaborate*object*unknown-val
   (state <s> ^name obj-analyzer
              ^object.mapping <mapping>)
   (<mapping> ^att-val-pair.val <val>)
   (<val> ^internal <internal-val>
         -^external <external-val>)
-->
   (<mapping> ^unknown-val <internal-val>)
}

