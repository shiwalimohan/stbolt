sp {create-object*elaborate*last-create-operator
   (state <s> ^name create-object
              ^superstate <ss>)
   (<ss> ^operator <cur-op>)
   (<cur-op> ^name create-object)
   -{(<ss> ^operator {<o> <> <cur-op>} +)
     (<o> ^name create-object)}
-->
   (<s> ^last-create-operator true)
}

sp {create-object*propose*comprehend-att
   (state <s> ^name create-object
              ^last-create-operator true
             -^processed.internal <attribute>
              ^top-state.object <obj>)
   (<obj> ^mapping.att-val-pair <av-pair>)
   (<av-pair> ^att-type internal
              ^att.internal <attribute>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name comprehend-att
        ^attribute <attribute>
        ^type internal)
}