sp {recognition-question*propose*complete-recognition-question
   (state <s> ^name recognition-question
              ^object-match <obj>)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-recognition-question
        ^object <obj>)
}

sp {recognition-question*apply*complete-recognition-question*mark-processed
   (state <s> ^name recognition-question
              ^operator.name complete-recognition-question
              ^superstate <ss>)
   (<ss> ^message <m>)
-->
   (<ss> ^processed <m>)
}

sp {recognition-question*apply*complete-recognition-question*point
   (state <s> ^name recognition-question
              ^io.output-link <ol>
              ^operator <o>)
   (<o> ^name complete-recognition-question
        ^object.id <obj-id>)
-->
   (<ol> ^point.object-id <obj-id>)
}
