sp {process-message*propose*process-same-unknown-att
   (state <s> ^name process-message
             -^processed <id>
              ^current-question.type same-unknown-att)
-->
   (<s> ^operator <o> +)
   (<o> ^name process-same-unknown-att)
}

sp {process-message*apply*process-same-unknown-att
   (state <s> ^name process-message
              ^operator.name process-same-unknown-att
              ^message.words.word <response>
              ^current-question.unknown-att <internal-att>
              ^top-state <top>)
-->
   (<top> ^learned-att <a>)
   (<a> ^internal <internal-att>
        ^external <response>)
}

sp {process-message*apply*process-same-unknown-att*mark-processed
   (state <s> ^name process-message
              ^message.id <msgID>
              ^operator.name process-same-unknown-att)
-->
   (<s> ^processed <msgID>)
}