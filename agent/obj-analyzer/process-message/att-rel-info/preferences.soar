sp {prefer*all*over*complete-att-rel-info
   (state <s> ^name att-rel-info
              ^operator <o> +)
   (<o> ^name complete-att-rel-info)
-->
   (<s> ^operator <o> <)
}

sp {prefer*init-att-rel-info*over*all
   (state <s> ^name att-rel-info
              ^operator <o> +)
   (<o> ^name init-att-rel-info)
-->
   (<s> ^operator <o> >)
}

sp {prefer*modify-object*over*copy-adjective
   (state <s> ^name att-rel-info
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name modify-object)
   (<o2> ^name copy-adjective)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {prefer*copy-adjective*over*comprehend-att
   (state <s> ^name att-rel-info
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name copy-adjective)
   (<o2> ^name comprehend-att)
-->
   (<s> ^operator <o1> > <o2>)
}