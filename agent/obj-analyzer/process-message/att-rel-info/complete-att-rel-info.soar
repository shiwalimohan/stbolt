sp {att-rel-info*propose*complete-att-rel-info
   (state <s> ^name att-rel-info)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-att-rel-info)
}

sp {att-rel-info*apply*complete-att-rel-info
   (state <s> ^name att-rel-info
              ^operator.name complete-att-rel-info
              ^superstate <ss>)
   (<ss> ^message <m>)
-->
   (<ss> ^processed <m>)
}