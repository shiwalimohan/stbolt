# smem-val-query
#   Queries smem for the matching value

sp {lookup-val*propose*smem-val-query
   (state <s> ^name lookup-val
              ^query.internal <internal-val>
              ^query.external <external-val>
              -^smem.command.query.type value)
-->
   (<s> ^operator <o> +)
   (<o> ^name smem-val-query)
}

sp {lookup-val*apply*smem-val-query
   (state <s> ^name lookup-val
              ^operator.name smem-val-query
              ^query.internal <internal-val>
              ^query.external <external-val>
              ^smem.command <cmd>)
-->
   (<cmd> ^query <q>)
   (<q> ^type value
        ^internal <internal-val>
        ^external <external-val>)
}
