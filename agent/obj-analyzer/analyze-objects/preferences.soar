sp {analyze-objects*reject*external*if*current-question
   (state <s> ^name analyze-objects
              ^top-state <top>
              ^operator <o> +)
   (<o> ^type external)
   (<top> ^current-question <cq>)
-->
   (<s> ^operator <o> -)
}

sp {analyze-objects*prefer*external*over*internal
   (state <s> ^name analyze-objects
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^type external)
   (<o2> ^type internal)
-->
   (<s> ^operator <o1> > <o2>)
}