sp {analyze-objects*propose*mark-changed-false
   (state <s> ^name analyze-objects
              ^object <obj>)
   (<obj> ^changed true)
   -{(<s> ^operator <o> +)
   (<o> ^object <obj>)}
-->
   (<s> ^operator <o> + = >)
   (<o> ^name mark-changed-false)
}

sp {analyze-objects*apply*mark-changed-false
   (state <s> ^name analyze-objects
              ^operator.name mark-changed-false
              ^object <obj>)
   -{(<s> ^operator <o> +)
     (<o> ^object <obj>)}
	(<obj> ^changed true)
-->
   (<obj> ^changed false true -)
}