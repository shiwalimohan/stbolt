# Prefer all other operators over complete-object-desc
sp {prefer*all*over*complete-object-desc
   (state <s> ^name object-desc
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name <> complete-object-desc
         ^category object-processing)
   (<o2> ^name complete-object-desc)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {prefer*modify-object*over*lookup-val2att
   (state <s> ^name object-desc
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name modify-object)
   (<o2> ^name lookup-val2att)
-->
   (<s> ^operator <o1> > <o2>)
}