# If the create-object operator on the superstate is the last one, mark it so
sp {create-object*elaborate*last-create-operator
   (state <s> ^name create-object
              ^superstate <ss>)
   (<ss> ^operator <cur-op>)
   (<cur-op> ^name create-object)
   -{(<ss> ^operator {<o> <> <cur-op>} +)
     (<o> ^name create-object)}
-->
   (<s> ^last-create-operator true)
}

sp {create-object*elaborate*internal-att
   (state <s> ^name create-object
              ^top.object.mapping.att-val-pair <av-pair>)
   (<av-pair> ^att-type internal
              ^att.internal <internal-att>)
-->
   (<s> ^internal-att <internal-att>)
}