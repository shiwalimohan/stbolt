# Creates the initial object structure in the substate
sp {create-object*propose*init-create-object
   (state <s> ^name create-object
             -^object <obj>
              ^superstate.operator <ss-op>)
   (<ss-op> ^name create-object
        	^id <id>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name init-create-object
        ^id <id>)
}

# Create the beginning object structure to start working on in the substate
sp {create-object*apply*init-create-object
   (state <s> ^name create-object
              ^io.input-link.objects.object <il-obj>
              ^operator <o>)
   (<il-obj> ^id <id>
             ^pose <pose>)
   (<o> ^name init-create-object
        ^id <id>)
-->
   (<s> ^object <obj>)
   (<obj> ^id <id>
          ^pose <pose>
          ^desc <desc>
          ^external-desc <e-desc>
          ^comparisons <comp>
          ^mapping <mapping>)
}

