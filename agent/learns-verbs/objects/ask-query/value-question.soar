# value-question
#	Query for an unknown value for a known attribute

sp {elaborate*ask-query*value-question*type-info
   (state <s> ^operator <o> +)
   (<o> ^name ask-query
        ^query.type value-question)
-->
   (<o> ^type value-question
        ^expected-response bare-value-response)
}

sp {elaborate*ask-query*value-question*fields
   (state <s> ^operator <o>)
   (<o> ^name ask-query
        ^query <q>
        ^fields <f>)
   (<q> ^type value-question
        ^object.external-desc <external-desc>
        ^att-val-pair.att.external <external-att>)
-->
   (<f> ^attribute-relation <rel>)
   (<rel> ^word <external-att>
          ^object1 <external-desc>)
}