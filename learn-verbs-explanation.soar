pushd ./topchef-soar
source all_source.soar
popd

pushd ./verb-learning
source all_source.soar
popd

pushd ./interaction
source all_source.soar
popd


sp {propose*init-agent
   (state <s> -^name
   	      ^superstate nil)
-->
   (<s> ^operator <op> + >)
   (<op> ^name init-agent)
}

sp {apply*init-agent
   (state <s> ^operator.name init-agent)
-->
   (<s> ^topstate <s>)
}